#include<iostream>
#include<string>
using namespace std;

int main() {
	string name;
	string pwd;
	string repwd;
	cout << "이름 입력:";
	getline(cin, name);

	cout << "암호 입력:";
	getline(cin, pwd);

	cout << "다시 입력:";
	getline(cin, repwd);

	if (pwd == repwd)
		cout << name << "님께서 로그인하셨습니다.";
	else
		cout << name << "님, 다시 입력하세요.";
}
