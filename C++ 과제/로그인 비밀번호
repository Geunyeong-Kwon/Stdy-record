#include<iostream>
#include<cstring>
using namespace std;

int main() {
	char name[50], pwd[50], repwd[50];
	cout << "이름 입력:";
	cin >> name;

	cout << "암호 입력:";
	cin >> pwd;

	cout << "다시 입력:";
	cin >> repwd;

	if (strcmp(pwd, repwd) == 0)
		cout << name << "님께서 로그인하셨습니다.";
	else
		cout << name << "님, 다시 입력하세요.";
}
